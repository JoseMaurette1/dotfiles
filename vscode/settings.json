{
    "workbench.settings.editor": "json",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.fontFamily": "CommitMono",
    "editor.fontLigatures": true,
    "editor.lineHeight": 1.9,
    "editor.lineNumbers": "relative",
    "editor.fontSize": 17,
    "terminal.integrated.fontSize": 17,
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        "**/node_modules": false,
        "**/.next": false,
        "**/.husky": false,
        "**/drizzle": false,
        "**/dist": false,
        "**/.todo.md": false,
        "**/.vscode": false,
        "**/.eslintrc.cjs": false,
        "**/.editorconfig": false,
        "**/.gitignore": false,
        "**/.lintstagedrc.mjs": false,
        "**/.markdownlint.json": false,
        "**/.markdownlintignore": false,
        "**/.prettierignore": false,
        "**/bun.lockb": false,
        "**/cspell.json": false,
        "**/package-lock.json": false,
        "**/pnpm-lock.yaml": false,
        "**/next-env.d.ts": false,
        "**/postcss.config.cjs": false,
        "**/prettier.config.js": false,
        "**/README.md": false,
        "**/next.config.js": false,
        "**/tailwind.config.ts": false,
        "**/drizzle.config.ts": false,
        "**/migrate.ts": false,
        "**/.env.example": false,
        "**/.eslintrc.json": false,
        "**/next.config.mjs": false,
        "**/tsconfig.json": false,
        "**/yarn.lock": false,
        "**/package.json": false,
        "**/postcss.config.mjs": false
    },
    "explorer.compactFolders": false,
    "editor.tabSize": 2,
    "breadcrumbs.filePath": "off",
    "files.trimTrailingWhitespace": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "window.zoomLevel": 0.5,
    "terminal.integrated.env.linux": {},
    "window.menuBarVisibility": "toggle",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.stickyScroll.enabled": false,
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "git.confirmSync": false,
    "workbench.productIconTheme": "",
    "debug.disassemblyView.showSourceCode": false,
    "remote.autoForwardPortsSource": "hybrid",
    "workbench.sideBar.location": "right",
    "window.commandCenter": false,
    "workbench.iconTheme": "vscode-jetbrains-icon-theme-2023-auto",
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.colorTheme": "Hybrid dim",
    "editor.bracketPairColorization.enabled": false,
    "editor.guides.bracketPairs": false
}
